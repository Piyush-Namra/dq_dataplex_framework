        "column": "bike_id",
        "threshold": 1,
        "ignore_null": false
      },
      {
        "range_expectation": {
          "min_value": "1800",
          "max_value": "6660",
          "strict_min_enabled": true,
          "strict_max_enabled": true
        },
        "dimension": "VALIDITY",
        "column": "duration"
      },
      {
        "uniqueness_expectation": {
  
        },
